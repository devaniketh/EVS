{
  "name": "subway-surfers-game-flow-evm",
  "version": "1.0.0",
  "description": "Subway Surfers Game Smart Contracts for Flow EVM",
  "main": "flow-integration.js",
  "scripts": {
    "deploy:testnet": "forge script script/DeployFlowEVM.s.sol --rpc-url https://testnet.evm.nodes.onflow.org --broadcast --verify --chain-id 545",
    "deploy:mainnet": "forge script script/DeployFlowEVM.s.sol --rpc-url https://mainnet.evm.nodes.onflow.org --broadcast --verify --chain-id 747",
    "test:flow": "forge script script/TestFlowEVM.s.sol --rpc-url https://testnet.evm.nodes.onflow.org",
    "test": "forge test",
    "build": "forge build",
    "clean": "forge clean",
    "example": "node flow-integration.js"
  },
  "keywords": [
    "flow",
    "evm",
    "blockchain",
    "game",
    "smart-contracts",
    "ethereum",
    "solidity"
